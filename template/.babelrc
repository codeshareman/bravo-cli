{
    "presets": [
        [ 
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "safari >= 7"
                    ]
                }
            }
        ],
         "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-syntax-dynamic-import"],
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }],
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose" : true }],
        [ 
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ],
    "env": {
        "test": {
          "presets": [
            "env"
          ],
          "plugins": [
            "istanbul"
          ]
        },
        "production": {
          "presets": [
            "react-optimize"
          ]
        }
    }
}